{
  "expo": {
    "name": "Bricolage Helper",
    "slug": "bricolage-helper",
    "version": "1.0.0",
    "orientation": "default",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bricolage.helper"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      },
      "package": "com.bricolage.helper"
    },
    "web": {
      "bundler": "metro"
    },
    "sdkVersion": "53.0.0",
    "platforms": ["ios", "android"]
  }
}